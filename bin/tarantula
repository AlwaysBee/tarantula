#!/usr/bin/env node

'ues strict'

const program = require('commander'),
        debug = {
          info: require("debug")("info")
        }

const version = (() => {
  try{
   return require("tarantula/package.json").version
  }catch(e){
   return 'dev'
  }
})()

program
  .version(version)
  .command("dispatch <server>", "tarantula client")
  .command("crawl <scriptPath>", "test the client script")
  .command("publish", "publish project")
  .parse(process.argv)
