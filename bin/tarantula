#!/usr/bin/env node

'ues strict'

const program = require('commander'),
  { version } = require("../package.json")

program
  .version(version)
  .command("server", "start a server")
  .command("dispatch <server>", "tarantula client")
  .command("dev", "for developer")
  .command("publish", "publish project")
  .parse(process.argv)
