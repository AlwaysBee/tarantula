#!/usr/bin/env node

'ues strict'

const program = require('commander'),
        debug = {
          info: require("debug")("info")
        }

let version = 'dev'
try {version = require("tarantula/package.json").version}catch(e){}

program
  .version(version)
  .command("dispatch <server>", "tarantula client")
  .command("crawler <scriptPath>", "test the client script")
  .command("publish", "publish project")
  .option('--init', "init")
  .parse(process.argv)

;(async () => {
  debug.info("fuck")
  // if (program.init){
  //
  // }
  //
  // program.help()
})()
